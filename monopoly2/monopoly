Private Sub Command13_Click()
If Text4.text = "1" Then
Form1.MousePointer = 2
Text2.text = "pl1"
Else
End If
If Text4.text = "2" Then
Form1.MousePointer = 3
Text2.text = "pl2"
Else
End If
Dim dice, Lowest, Highest As Integer
Lowest = 1
Highest = 12
Randomize
dice = Int((Highest - Lowest + 1) * Rnd + Lowest)
If dice <= 12 Then
Let Text23.text = dice
Let Text110.text = dice
Else
MsgBox "Not More than Two Dices should be used", vbDefaultButton1
End
End If
MsgBox "Dice thrown", vbDefaultButton1
Text23.text = ""
