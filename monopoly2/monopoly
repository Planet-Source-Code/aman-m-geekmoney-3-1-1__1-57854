If d = 1 Then
MsgBox "Property Values Average"
MsgBox "Sold Card for Rs.500"
Text1.text = Text1.text + 500
Command3.Caption = "     Mumbai"
Command3.FontBold = False
Command33.Visible = False
Else
End If
