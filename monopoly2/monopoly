Private Sub Command25_Click()
On Error Resume Next
Dim pl1 As String
Dim pl2 As String
Dim pl3 As String
Dim pl4 As String
If Text2.text = "" Then
MsgBox "Please select a player(pl1/pl2)and then press the button"
Else
End If
If Text2.text = "pl1" Then
MsgBox "Moving      " & Text2.text
Else
End If
If Text2.text = "pl2" Then
MsgBox "Moving      " & Text2.text
Else
End If
If Text2.text = "PL1" Or Text2.text = "PL2" Then
MsgBox "Write in small letters"
Else
End If
End Sub
Private Sub Command26_Click()
On Error Resume Next
If Text4.text = "1" Then
Dim loan As Double
Let Text8.text = "20000"
Let loan = Text8.text
Dim simple, rate, Lowest3, Highest3 As Integer
Low3 = 1
High3 = 7
Randomize
Text99.text = ""
Text1001.text = ""
rate = Int((High3 - Low3 + 1) * Rnd + Low3)
simple = loan * rate * 10 * 0.001
Let Text99.text = simple
Text1.text = Text1.text + loan
Let Text1001 = rate
Command26.Enabled = False
Else
MsgBox "Loan Rejected"
End If
End Sub
Private Sub Command27_Click()
On Error Resume Next
Shape17.Visible = True
Shape18.Visible = True
Command1.Enabled = False
Command1.Visible = False
Command27.Enabled = True
Command27.Visible = True
If Text1.text < 0 Then
MsgBox "Game Over", vbCritical
End
Else
End If
MsgBox "Get Your Paycheck Rs.1800"
If Text4.text = "1" Then
Text1.text = Text1.text + 1800
If Command26.Enabled = False Then
Text1.text = Text1.text - Text99.text
Else
End If
If Command29.Enabled = False Then
Text1.text = Text1.text - Text100.text
Else
End If
Else
End If
If Text4.text = "2" Then
Text3.text = Text3.text + 1800
If Command30.Enabled = False Then
Text3.text = Text3.text - Text101.text
Else
End If
If Command31.Enabled = False Then
Text3.text = Text3.text - Text102.text
Else
End If
Else
End If
On Error Resume Next
End Sub

Private Sub Command28_Click()
On Error Resume Next
Dim score1, score2 As Integer
score1 = Text1.text
score2 = Text3.text
If score1 > score2 Then
MsgBox "Player 1 WON", vbExclamation
Else
End If
If score1 < score2 Then
MsgBox "Player 2 WON", vbExclamation
Else
End If
If score1 = score2 Then
MsgBox "No Wins or Losses", vbInformation
Else
End If
End
On Error Resume Next
End Sub

Private Sub Command29_Click()
On Error Resume Next
If Text4.text = "1" Then
Dim loan As Double
Let Text6.text = "5000"
Let loan = Text6.text
Dim simple, rate, Low3, High3 As Integer
Low3 = 1
High3 = 7
Randomize
Text100.text = ""
Text1000.text = ""
rate = Int((High3 - Low3 + 1) * Rnd + Low3)
Text1.text = Text1.text + loan
simple = loan * rate * 10 * 0.01
Let Text1000 = rate
Let Text100.text = simple
Command29.Enabled = False
Else
MsgBox "Loan Rejected"
End If
Command29.Enabled = False
On Error Resume Next
End Sub

