MsgBox "Property Values Low"
MsgBox "Sold Card for Rs.100"
Text1.text = Text1.text + 100
Command3.Caption = "     Mumbai"
Command3.FontBold = False
Command33.Visible = False
Else
End If
If d = 4 Then
MsgBox "Property Values High"
MsgBox "Sold Card for Rs.1500"
Text1.text = Text1.text + 1500
Command3.Caption = "     Mumbai"
Command3.FontBold = False
Command33.Visible = False
Else
End If
List1.AddItem "Mumbai sold"
Else
End If
Else
MsgBox "Invalid move", vbCritical, "Invalid"
End If
On Error Resume Next
End Sub

