Private Sub Command10_Click()
Text1.Locked = True
Text5.text = "I would pay as soon as possible to be Free."
If Text2.text = "pl1" Then
MsgBox " Pl1 is in JAIL                PAY Rs.1800 TO BE FREE"
Text1.text = Text1.text - 1800
Text90.Visible = False
Text90.Enabled = False
Label3.Visible = False
Text90.text = ""
Else
End If
If Text2.text = "pl2" Then
MsgBox " Pl2 is in JAIL                PAY Rs.1800 TO BE FREE"
Text3.text = Text3.text - 1800
Text90.Visible = False
Text90.Enabled = False
Label3.Visible = False
Text90.text = ""
Else
End If
End Sub

