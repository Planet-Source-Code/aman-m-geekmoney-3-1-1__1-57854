Private Sub Timer1_Timer()
On Error Resume Next
Dim mark, d, e As Integer
d = 1
e = 3
Randomize
mark = Int((e - d + 1) * Rnd + e)
If mark = 1 Then
Dim market, a, b As Integer
a = 1
b = 7
Randomize
market = Int((b - a + 1) * Rnd + a)
Let Text10.text = ""
If market = 1 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "ABM  +29.60"
Else
End If
If market = 2 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "GBM  +09.60"
Else
End If
If market = 3 Then
Text10.ForeColor = &HFF&
Text10.text = "Mbase -6.01"
Else
End If
If market = 4 Then
Text10.ForeColor = &HFF&
Text10.text = "CorOda -6.01"
Else
End If
If market = 5 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "A++ Interactive  +49.60"
Else
End If
If market = 6 Then
Text10.ForeColor = &HFF&
Text10.text = "B-- -6.01"
Else
End If
If market = 7 Then
Text10.ForeColor = &HFF&
Text10.text = "SJ Pvt. Ltd -6.01"
Else
End If
Else
End If
If mark = 2 Then
Text10.text = "No Information Currently Available"
Else
End If
If mark = 3 Then
Dim m, R, L As Integer
R = 1
L = 9
Randomize
m = Int((L - R + 1) * Rnd + R)
If m = 1 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "Steel++  +29.60"
Else
End If
If m = 2 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "Gomber++  +09.60"
Else
End If
If m = 3 Then
Text10.ForeColor = &HFF&
Text10.text = "Mdase -6.01"
Else
End If
If m = 4 Then
Text10.ForeColor = &HFF&
Text10.text = "Coya -6.01"
Else
End If
If m = 5 Then
Text10.ForeColor = (&HFF00&)
Text10.text = "A++ Interactive  +49.60"
Else
End If
If m = 6 Then
Text10.ForeColor = &HFF&
Text10.text = "tuyusa -6.01"
Else
End If
If m = 7 Then
Text10.ForeColor = &HFF&
Text10.text = "RMLWX Pvt. Ltd -6.01"
Else
End If
If m = 8 Then
Text10.ForeColor = &HFF&
Text10.text = " RM Lal and Sons. Pvt. Ltd. -0.09"
Else
End If
If m = 9 Then
Text10.ForeColor = (&HFF00&)
Text10.text = " DM construction Pvt. Ltd. +100.05"
Else
End If
Else
End If
On Error Resume Next
End Sub
